    "downloadjs": "^1.4.7",
    "is-mobile": "^4.0.0",
    "prop-types": "^15.7.2",
    "rc-slider": "^10.6.2",
    "rc-switch": "^4.1.0",
    "react-draggable": "^4.4.3",
    "react-icons": "^5.5.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.10",
    "sortablejs": "^1.15.2"
  },
